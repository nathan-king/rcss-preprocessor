{
  "$schema": "vscode://schemas/color-theme",
  "name": "RCSS Accent (Catppuccin)",
  "type": "dark",
  "tokenColors": [
    {
      "scope": "rcss.variable",
      "settings": { "foreground": "#B5E8EF" }
    },
    {
      "scope": "rcss.selector.variable",
      "settings": { "foreground": "#B5E8EF" }
    },
    {
      "scope": "rcss.variable.sigil",
      "settings": { "foreground": "#7BCCF4" }
    },
    {
      "scope": "rcss.token",
      "settings": { "foreground": "#A6E3A1" }
    },
    {
      "scope": "rcss.token.sigil",
      "settings": { "foreground": "#94E2D5" }
    },
    {
      "scope": "rcss.preset",
      "settings": { "foreground": "#CBA6F7" }
    }
  ]
}
