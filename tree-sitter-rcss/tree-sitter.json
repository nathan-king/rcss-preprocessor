{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "rcss",
      "camelcase": "Rcss",
      "title": "RCSS",
      "scope": "source.rcss",
      "file-types": [
        "rcss"
      ],
      "injection-regex": "^rcss$",
      "class-name": "TreeSitterRcss"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A Tree-sitter grammar for the RCSS stylesheet language.",
    "authors": [
      {
        "name": "Nathan King",
        "email": "hello@nathanking.io",
        "url": "https://github.com/nathan-king"
      }
    ],
    "links": {
      "repository": "https://github.com/nathan-king/rcss-preprocessor"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}