{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "rcss",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "rule"
      }
    },
    "rule": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "selector"
        },
        {
          "type": "SYMBOL",
          "name": "block"
        }
      ]
    },
    "selector": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "class_selector"
            },
            {
              "type": "SYMBOL",
              "name": "id_selector"
            },
            {
              "type": "SYMBOL",
              "name": "element_selector"
            }
          ]
        }
      ]
    },
    "class_selector": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "."
        },
        {
          "type": "PATTERN",
          "value": "[a-zA-Z_][a-zA-Z0-9_-]*"
        }
      ]
    },
    "id_selector": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "PATTERN",
          "value": "[a-zA-Z_][a-zA-Z0-9_-]*"
        }
      ]
    },
    "element_selector": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_-]*"
    },
    "block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "declaration"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "property"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "value"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "property": {
      "type": "PATTERN",
      "value": "[a-zA-Z-]+"
    },
    "value": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "token"
          },
          {
            "type": "SYMBOL",
            "name": "number"
          },
          {
            "type": "SYMBOL",
            "name": "color"
          },
          {
            "type": "SYMBOL",
            "name": "identifier"
          }
        ]
      }
    },
    "token": {
      "type": "PATTERN",
      "value": "@[a-zA-Z0-9_-]+(\\/[0-9.]+)?"
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+(\\.\\d+)?(rem|px|%|em)?"
    },
    "color": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "#[0-9a-fA-F]{3,6}"
        },
        {
          "type": "PATTERN",
          "value": "oklch\\([^)]*\\)"
        },
        {
          "type": "PATTERN",
          "value": "rgba?\\([^)]*\\)"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_-]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}